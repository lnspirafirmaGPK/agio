import time
from enum import Enum
from dataclasses import dataclass
from typing import List, Dict, Any, Optional

# --- 1. ‡∏ô‡∏¥‡∏¢‡∏≤‡∏°‡∏ß‡∏±‡∏ï‡∏ñ‡∏∏‡∏î‡∏¥‡∏ö‡∏ó‡∏≤‡∏á‡∏ò‡∏£‡∏£‡∏°‡∏ä‡∏≤‡∏ï‡∏¥ (Raw Nature Definitions) ---

class NatureType(Enum):
    SEA = "The Broad Main / Roaring Tyranny"
    SKY = "The Purple-colour'd Face / Weeping Morn"
    GARDEN = "Blooming Apricocke / Rich Crop"

@dataclass
class NaturalPhenomenon:
    type: NatureType
    raw_description: str
    intensity: float  # 0.0 to 1.0 (Chaos Level)

# --- 2. ‡πÄ‡∏Ñ‡∏£‡∏∑‡πà‡∏≠‡∏á‡∏à‡∏±‡∏Å‡∏£‡∏õ‡∏£‡∏∞‡∏°‡∏ß‡∏•‡∏ú‡∏• (The Processing Engine) ---

class NatureToCodeEngine:
    """
    ‡πÄ‡∏Ñ‡∏£‡∏∑‡πà‡∏≠‡∏á‡∏à‡∏±‡∏Å‡∏£‡∏ó‡∏µ‡πà‡∏ó‡∏≥‡∏´‡∏ô‡πâ‡∏≤‡∏ó‡∏µ‡πà‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô '‡∏Ñ‡∏ß‡∏≤‡∏°‡∏õ‡∏±‡πà‡∏ô‡∏õ‡πà‡∏ß‡∏ô‡∏ó‡∏≤‡∏á‡∏ò‡∏£‡∏£‡∏°‡∏ä‡∏≤‡∏ï‡∏¥' 
    ‡πÉ‡∏´‡πâ‡πÄ‡∏õ‡πá‡∏ô '‡∏£‡∏´‡∏±‡∏™‡πÇ‡∏Ñ‡∏£‡∏á‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏ó‡∏µ‡πà‡∏™‡∏á‡πà‡∏≤‡∏á‡∏≤‡∏°' (Structured Elegance)
    """

    def __init__(self, processing_speed: str = "20X_ACCELERATOR"):
        self.processing_speed = processing_speed
        print(f"üöÄ Engine Started: Mode {self.processing_speed}")
        print("   Ready to marshal Reason to the point of human skill.\n")

    def _calm_the_sea(self, raw_input: str) -> Dict[str, Any]:
        """
        ‡∏õ‡∏£‡∏∞‡∏°‡∏ß‡∏•‡∏ú‡∏• '‡∏ó‡πâ‡∏≠‡∏á‡∏ó‡∏∞‡πÄ‡∏•‡πÅ‡∏´‡πà‡∏á‡∏≠‡∏≤‡∏£‡∏°‡∏ì‡πå':
        ‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏à‡∏≤‡∏Å 'Tempest' (‡∏û‡∏≤‡∏¢‡∏∏) -> 'Graceful Wave' (‡∏Ñ‡∏•‡∏∑‡πà‡∏ô‡∏ó‡∏µ‡πà‡∏™‡∏á‡πà‡∏≤‡∏á‡∏≤‡∏°)
        """
        # Logic: ‡∏•‡∏î‡∏ó‡∏≠‡∏ô‡∏Ñ‡∏ß‡∏≤‡∏°‡∏ß‡∏∏‡πà‡∏ô‡∏ß‡∏≤‡∏¢ ‡πÄ‡∏´‡∏•‡∏∑‡∏≠‡πÄ‡∏û‡∏µ‡∏¢‡∏á Motion ‡∏ó‡∏µ‡πà‡∏à‡∏≥‡πÄ‡∏õ‡πá‡∏ô (Singular in each particular)
        return {
            "origin": "Roaring Tyranny",
            "processed_state": "Graceful Wave",
            "action": "Move still, still so",
            "function": "Own no other function (Pure Aesthetic/Logic)"
        }

    def _marshal_reason(self, raw_input: str) -> Dict[str, Any]:
        """
        ‡∏õ‡∏£‡∏∞‡∏°‡∏ß‡∏•‡∏ú‡∏• '‡∏ó‡πâ‡∏≠‡∏á‡∏ü‡πâ‡∏≤‡πÅ‡∏•‡∏∞‡πÅ‡∏™‡∏á‡∏™‡∏ß‡πà‡∏≤‡∏á':
        ‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏à‡∏≤‡∏Å 'Weeping Morn' -> 'Clear Judgment' (‡∏Ñ‡∏ß‡∏≤‡∏°‡∏à‡∏£‡∏¥‡∏á‡∏ó‡∏µ‡πà‡∏õ‡∏£‡∏≤‡∏Å‡∏è‡∏ä‡∏±‡∏î)
        """
        # Logic: ‡πÉ‡∏ä‡πâ‡πÄ‡∏´‡∏ï‡∏∏‡∏ú‡∏•‡πÄ‡∏õ‡πá‡∏ô‡πÅ‡∏°‡πà‡∏ó‡∏±‡∏û (Reason as marshal)
        return {
            "origin": "Weeping Morn / Violet Past",
            "processed_state": "Open Truth",
            "audit_result": "Daylight discovers not more",
            "decision": "Valid"
        }

    def _distill_garden(self, raw_input: str) -> Dict[str, Any]:
        """
        ‡∏õ‡∏£‡∏∞‡∏°‡∏ß‡∏•‡∏ú‡∏• '‡∏™‡∏ß‡∏ô‡πÅ‡∏•‡∏∞‡∏õ‡πà‡∏≤':
        ‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏à‡∏≤‡∏Å 'Mixed Crop' -> 'Essence' (‡πÅ‡∏Å‡πà‡∏ô‡πÅ‡∏ó‡πâ)
        """
        # Logic: ‡∏£‡∏ß‡∏ö‡∏£‡∏ß‡∏°‡∏ä‡∏¥‡πâ‡∏ô‡∏™‡πà‡∏ß‡∏ô‡∏ó‡∏µ‡πà‡∏Å‡∏£‡∏∞‡∏à‡∏±‡∏î‡∏Å‡∏£‡∏∞‡∏à‡∏≤‡∏¢ (Combined Sums)
        return {
            "origin": "Rich Crop of Sea and Land",
            "processed_state": "Distilled Essence",
            "value": "Pleasures of the World (Optimized)",
            "structure": "Silken Terms Precise" # ‡∏£‡∏π‡∏õ‡πÅ‡∏ö‡∏ö‡∏ó‡∏µ‡πà‡∏ß‡∏¥‡∏à‡∏¥‡∏ï‡∏£‡∏ö‡∏£‡∏£‡∏à‡∏á
        }

    def manifest_awesome(self, phenomena: List[NaturalPhenomenon]) -> Dict[str, Any]:
        """
        ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏´‡∏•‡∏±‡∏Å: ‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏Ñ‡∏ß‡∏≤‡∏°‡∏£‡∏π‡πâ‡∏™‡∏∂‡∏Å '‡∏™‡∏∏‡∏î‡∏¢‡∏≠‡∏î‡∏°‡∏≤‡∏Å‡πÄ‡∏•‡∏¢' (The Awesome Feeling)
        ‡πÇ‡∏î‡∏¢‡∏Å‡∏≤‡∏£ Loop ‡∏ú‡πà‡∏≤‡∏ô‡∏ò‡∏£‡∏£‡∏°‡∏ä‡∏≤‡∏ï‡∏¥‡πÅ‡∏•‡∏∞‡πÅ‡∏õ‡∏•‡∏á‡πÄ‡∏õ‡πá‡∏ô Code ‡∏ó‡∏µ‡∏•‡∏∞‡∏™‡πà‡∏ß‡∏ô
        """
        start_time = time.time()
        structured_output = {}

        print("‚ö° Processing Nature stream...")

        for p in phenomena:
            if p.type == NatureType.SEA:
                structured_output["emotion_flow"] = self._calm_the_sea(p.raw_description)
            elif p.type == NatureType.SKY:
                structured_output["logic_core"] = self._marshal_reason(p.raw_description)
            elif p.type == NatureType.GARDEN:
                structured_output["data_beauty"] = self._distill_garden(p.raw_description)

        # Audit ‡πÄ‡∏ß‡∏•‡∏≤‡∏ó‡∏µ‡πà‡πÉ‡∏ä‡πâ (High Speed Processing)
        elapsed = time.time() - start_time
        
        return {
            "status": "MAGNIFICENT (‡∏™‡∏∏‡∏î‡∏¢‡∏≠‡∏î‡∏°‡∏≤‡∏Å‡πÄ‡∏•‡∏¢)",
            "audit_time": f"{elapsed:.6f} sec (Instant Way)",
            "payload": structured_output,
            "final_thought": "Nothing else to be done, see him. (Structure Complete)"
        }

# --- 3. ‡∏Å‡∏≤‡∏£‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô‡∏à‡∏£‡∏¥‡∏á (Execution) ---

# ‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏ß‡∏±‡∏ï‡∏ñ‡∏∏‡∏î‡∏¥‡∏ö‡∏ò‡∏£‡∏£‡∏°‡∏ä‡∏≤‡∏ï‡∏¥ (Input: Abstract/Chaos)
inputs = [
    NaturalPhenomenon(NatureType.SEA, "Tempest and roaring waters", 0.9),
    NaturalPhenomenon(NatureType.SKY, "Stars like aglets in the dark", 0.5),
    NaturalPhenomenon(NatureType.GARDEN, "Wild apricot and confusing flowers", 0.7)
]

# ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô Engine
engine = NatureToCodeEngine()

# ‡∏õ‡∏£‡∏∞‡∏°‡∏ß‡∏•‡∏ú‡∏•‡πÅ‡∏•‡∏∞‡∏£‡∏±‡∏ö‡∏ú‡∏•‡∏•‡∏±‡∏û‡∏ò‡πå
result = engine.manifest_awesome(inputs)

# ‡πÅ‡∏™‡∏î‡∏á‡∏ú‡∏•‡∏•‡∏±‡∏û‡∏ò‡πå‡πÅ‡∏ö‡∏ö JSON (Silken Precise Form)
import json
print("\nUser Perception (The Code):")
print(json.dumps(result, indent=2))